{
    "id": "awt.xms.photoshop",
    "name": "xMedia Publisher",
    "version": "1.0.0",
    "main": "index.html",
    "manifestVersion": 5,
    "host": {
        "app": "PS",
        "minVersion": "25.12.3"
    },
    "featureFlags": {
        "enableSWCSupport": true
    },
    "requiredPermissions": {
        "allowCodeGenerationFromStrings": true,
        "network": {
            "domains": []
        },
        "launchProcess": {
            "schemes": ["https"]
        },
        "localFileSystem": "fullAccess",
        "clipboard": "readAndWrite"
    },
    "entrypoints": [
        {
            "type": "panel",
            "id": "app",
            "label": {
                "default": "xMedia Publisher"
            },
            "minimumSize": {
                "width": 450,
                "height": 768
            },
            "maximumSize": {
                "width": 650,
                "height": 1024
            },
            "icons": [
                {
                    "width": 23,
                    "height": 23,
                    "path": "icon.svg"
                }
            ]
        }
    ],
    "icons": [
        {
            "width": 23,
            "height": 23,
            "path": "icon.svg",
            "theme": ["darkest", "dark", "light", "lightest"]
        }
    ]
}
